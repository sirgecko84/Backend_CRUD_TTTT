<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Nhap thong tin Lop</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.slim.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
    <div class="container">
        <div class="row">
            <div class="col-12 mt-3">Nhap thong tin lop</div>
            <form action="/post-crudClass" method="POST">
                <div class="form-group">
                    <label for="exampleInputPassword1">MaLop</label>
                    <input type="text" class="form-control" name="maLop" placeholder="1234">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Ten Lop</label>
                    <input type="text" class="form-control" placeholder="N1cl1" name="tenLop">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Ngay Khai Giang</label>
                    <input type="date" class="form-control" name="ngayKhaiGiang">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Ngay Ket Thuc</label>
                    <input type="date" class="form-control" name="ngayKetThuc">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Si so</label>
                    <input type="integer" class="form-control" name="siSo" placeholder="123456789">
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>


</body>

</html>